{{#each locations as |location|}}
  <tr class="location-row" data-id="{{location.id}}">
      <td>{{location.name}}</td>
      <td>{{location.lat}}</td>
      <td>{{location.long}}</td>
      <td>{{location.default}}</td>
      <td><button type="button" class="btn-line clickable check-weather-btn">Check weather</button></td>
      <td><button type="button" class="btn-line clickable edit-btn">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true" ></span>
          </button>
      </td>
      <td><button type="button" class="btn-line clickable delete-btn">
        <span class="glyphicon glyphicon-trash" aria-hidden="true" ></span>
          </button>
      </td>

  </tr>
{{/each}}
